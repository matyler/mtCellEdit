#!/bin/bash


. ./mtConf.txt


APP_NAME="mtNush"
BIN_NAME="mtnush"

MTCF_INIT

MTCF_ENABLE_QT
MTCF_DESKTOP
MTCF_MAN_PAGE

MTCF_PARSE_ARGS "$@"

MTCF_PRINT_HELP
MTCF_PRE_CONF

MTCF_CHECK_LIBS mtkit mtpixy mtdatawell gmp mpfr stdc++
MTCF_VERSION_BIN_NAME

MTCF_ENABLE_QT_QEX

MTCF_ADD_APPIMAGE_DATA

MTCF_POST_CONF
MTCF_PRINT_CONF
