#!/bin/bash


. ./mtConf.txt


APP_NAME="mtCrul"
BIN_NAME="mtcrul"

MTCF_INIT

MTCF_ENABLE_QT
MTCF_DESKTOP
MTCF_MAN_PAGE

MTCF_PARSE_ARGS "$@"

MTCF_PRINT_HELP
MTCF_PRE_CONF

MTCF_CHECK_LIBS mtkit mtpixy mtgin mtdatawell stdc++
MTCF_PKGCONFIG "gl"

MTCF_ENABLE_QT_QEX
MTCF_VERSION_BIN_NAME

MTCF_ADD_APPIMAGE_DATA

MTCF_POST_CONF
MTCF_PRINT_CONF
