#!/bin/bash


. ./mtConf.txt


APP_NAME="mtPixy"
BIN_NAME="mtpixy"

MTCF_INIT

MTCF_ENABLE_QT
MTCF_HANDBOOK
MTCF_DESKTOP
MTCF_MAN_PAGE

MTCF_PARSE_ARGS "$@"

MTCF_PRINT_HELP
MTCF_PRE_CONF

MTCF_CHECK_LIBS mtkit mtpixy stdc++

MTCF_ENABLE_QT_QEX
MTCF_VERSION_BIN_NAME

DATA_NAME=$BIN_NAME

MTCF_ADD_APPIMAGE_DATA
MTCF_ADD_DATA_NAME

MTCF_MAKE_DIRS_ADD "data"

MTCF_POST_CONF
MTCF_PRINT_CONF
