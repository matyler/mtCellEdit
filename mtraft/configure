#!/bin/bash


. ./mtConf.txt


APP_NAME="mtRaft"
BIN_NAME="mtraft"

MTCF_INIT

MTCF_ENABLE_QT
MTCF_DESKTOP
MTCF_MAN_PAGE

MTCF_PARSE_ARGS "$@"

MTCF_PRINT_HELP
MTCF_PRE_CONF

MTCF_CHECK_LIBS mtkit mtpixy mtcelledit stdc++
MTCF_VERSION_BIN_NAME

MTCF_ENABLE_QT_QEX

MTCF_ADD_BIN_FLAGS "-D_FILE_OFFSET_BITS=64"
MTCF_ADD_APPIMAGE_DATA

MTCF_POST_CONF
MTCF_PRINT_CONF
